<?xml version="1.0" encoding="UTF-8"?>


  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <mapper namespace="com.wolen.randomday.domestic.dao.DomesticDAO">
  
  	<select id="selectMenu" parameterType="int" resultType="com.wolen.randomday.domestic.model.MenuCategory">
  		SELECT
  			`id`
  			, `name`
  		FROM
  			`menuCategory`
  		WHERE
  			`id` = #{randomNumber}
  	</select>

	
	<select id="selectMenuNameById" parameterType="int" resultType="com.wolen.randomday.domestic.model.MenuCategory">
  		SELECT
  			`id`
  			, `name`
  		FROM
  			`menuCategory`
  		WHERE
  			`id` = #{menuId}
  	</select>


  </mapper>