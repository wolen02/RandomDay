<?xml version="1.0" encoding="UTF-8"?>

  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
  <mapper namespace="com.wolen.randomday.domestic.dao.DomesticDAO">
  	
  	<select id="selectGuList" parameterType="int"
  	resultType="com.wolen.randomday.domestic.model.Gu">
  		SELECT
  			`id`
  			, `doOrSiId`
  			, `name`
  		FROM
  			`gu`
  		WHERE
  			`doOrSiId` = #{doOrSiId}
  	
  	
  	</select>
  	
  	
  	<select id="selectGuIdByName" parameterType="String"
		resultType="int">
	  		SELECT
	  			`id`
	  		FROM
	  			`gu`
	  		WHERE
	  			`name` = #{name}
	  	
  	</select>
  	
  	
  	
  	  	<select id="selectGuNameById" parameterType="int"
		resultType="com.wolen.randomday.domestic.model.Gu">
	  		SELECT
  			`id`
  			, `doOrSiId`
  			, `name`
  			FROM
	  			`gu`
	  		WHERE
	  			`id` = #{guId}
	  	
  	</select>
  	
  	
  </mapper>



